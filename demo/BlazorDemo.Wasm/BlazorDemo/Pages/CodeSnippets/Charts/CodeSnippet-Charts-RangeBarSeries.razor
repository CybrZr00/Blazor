<CodeSnippetTabbed DocLink="https://docs.devexpress.com/Blazor/DevExpress.Blazor.DxChartRangeBarSeries-3">
    <CodeSnippetTabPage Text="Razor">@(@"<DxChart Data=""@WeatherForecasts""
         CssClass=""mw-1100"">
    <DxChartRangeBarSeries ValueField=""@((WeatherForecast i) => i.TemperatureF)"" ArgumentField=""@(i => i.Date.Date)"" Name=""Temperature, F"" />
    <DxChartTooltip>
        @context.GetRangePoint().Render((rangePoint) =>
            @<div class=""p-3"">
                <p>Max: @rangePoint.EndValue</p>
                <p>Min: @rangePoint.StartValue</p>
            </div>
        )
    </DxChartTooltip>
</DxChart>")</CodeSnippetTabPage>
</CodeSnippetTabbed>
